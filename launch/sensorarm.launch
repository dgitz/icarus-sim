<!--xml-->
<launch>
<node name="$(env ROS_HOSTNAME)_sensorarm_controller_node" pkg="icarus_sim" type="sensorarm_controller_node"  output="screen" clear_params="true">
        <param name ="/use_sim_time" value="true"/>
        <param name="startup_delay"       value="0.0"/>
        <param name="verbosity_level"       value="DEBUG"/>
        <param name="require_pps_to_start"  value="false"/> 
        <param name="startup_delay"       value="0.0"/>
	    <param name="verbosity_level"       value="NOTICE"/>
        <param name="require_pps_to_start"  value="false"/>    
        <param name="loop1_rate"            value="1"/>    
        <param name="loop2_rate"            value="10"/> 
        <param name="holdtime_step"         value="0.1"/>
        <param name="joint0_start_angle"     value="0.0"/>
        <param name="joint0_stop_angle"      value="6.28"/>
        <param name="joint0_step_angle"      value="0.1"/>
        
        <param name="joint1_start_angle"     value="0.0"/>
        <param name="joint1_stop_angle"      value="6.28"/>
        <param name="joint1_step_angle"      value="0.1"/>

        <param name="joint2_start_angle"     value="0.0"/>
        <param name="joint2_stop_angle"      value="6.28"/>
        <param name="joint2_step_angle"      value="0.1"/>

</node>
</launch>
